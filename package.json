    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^9.32.0",
